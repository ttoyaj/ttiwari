// Online C++ compiler to run C++ program online
#include <iostream>
#include <bits/stdc++.h>
using namespace std;


int factorial(int numb)
{
    if(numb>1)
    {
        return numb * factorial(numb-1);
    }
    else
    {
        return 1;
    }

}
int main() {
    // Write C++ code here
    std::cout << "Hello world! \n";
    int a;
    std::cout << "enter number for factorial \n";
    cin >> a ;
    int result ;
    result = factorial(a) ;

    cout << "Factorial is " << result ; 
    return 0;
}
